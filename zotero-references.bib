@article{kumarHighThroughputMethodIllumina2012,
  title = {A {{High-Throughput Method}} for {{Illumina RNA-Seq Library Preparation}}},
  author = {Kumar, Ravi and Ichihashi, Yasunori and Kimura, Seisuke and Chitwood, Daniel H. and Headland, Lauren R. and Peng, Jie and Maloof, Julin N. and Sinha, Neelima R.},
