  author = {Chini, Claudia C.S. and Zeidler, Julianna D. and Kashyap, Sonu and Warner, Gina and Chini, Eduardo Nunes},
  date = {2021-06},
  journaltitle = {Cell Metabolism},
  shortjournal = {Cell Metabolism},
