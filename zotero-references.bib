@article{yangEmbryonicMesodermalDefects1993,
  title = {Embryonic Mesodermal Defects in Alpha 5 Integrin-Deficient Mice},
  author = {Yang, J.T. and Rayburn, H. and Hynes, R.O.},
  date = {1993-12-01},
