@article{rauchTranscriptionalNetworksControlling2021,
  title = {Transcriptional Networks Controlling Stromal Cell Differentiation},
  author = {Rauch, Alexander and Mandrup, Susanne},
  date = {2021-07},
  journaltitle = {Nature Reviews Molecular Cell Biology},
  shortjournal = {Nat Rev Mol Cell Biol},
  volume = {22},
  number = {7},
  pages = {465--482},
  issn = {1471-0072, 1471-0080},
  doi = {10.1038/s41580-021-00357-7},
  url = {http://www.nature.com/articles/s41580-021-00357-7},
  urldate = {2022-03-11},
  abstract = {Abstract | Stromal progenitors are found in many different tissues, where they play an important role in the maintenance of tissue homeostasis owing to their ability to differentiate into parenchymal cells. These progenitor cells are differentially pre-programmed by their tissue microenvironment but, when cultured and stimulated in~vitro, these cells — commonly referred to as mesenchymal stromal cells (MSCs) — exhibit a marked plasticity to differentiate into many different cell lineages. Loss-of- function studies in~vitro and in~vivo have uncovered the involvement of specific signalling pathways and key transcriptional regulators that work in a sequential and coordinated fashion to activate lineage-selective gene programmes. Recent advances in omics and single-cell technologies have made it possible to obtain system-wide insights into the gene regulatory networks that drive lineage determination and cell differentiation. These insights have important implications for the understanding of cell differentiation, the contribution of stromal cells to human disease and for the development of~cell- based therapeutic applications.},
  langid = {english},
  keywords = {archived,REVIEW},
  annotation = {https://web.archive.org/web/20220311210634/https://www.nature.com/articles/s41580-021-00357-7},
  file = {/Users/vannijnatten/Zotero/storage/DXF5YDFT/Rauch_Mandrup_2021_Transcriptional networks controlling stromal cell differentiation.pdf}
}

@article{raudvereProfilerWebServer2019,
  title = {G:{{Profiler}}: A Web Server for Functional Enrichment Analysis and Conversions of Gene Lists (2019 Update)},
  shorttitle = {G},
  author = {Raudvere, Uku and Kolberg, Liis and Kuzmin, Ivan and Arak, Tambet and Adler, Priit and Peterson, Hedi and Vilo, Jaak},
