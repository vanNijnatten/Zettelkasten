  author = {Lam, David Chi-Leung and Luo, Susan Yang and Fu, Kin-Hang and Lui, Macy Mei-Sze and Chan, Koon-Ho and Wistuba, Ignacio Ivans and Gao, Boning and Tsao, Sai-Wah and Ip, Mary Sau-Man and Minna, John Dorrance},
