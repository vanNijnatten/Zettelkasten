@article{barnesChronicLungDiseases2021,
  title = {Chronic Lung Diseases: Prospects for Regeneration and Repair},
  shorttitle = {Chronic Lung Diseases},
  author = {Barnes, Peter J. and Anderson, Gary P. and Fagerås, Malin and Belvisi, Maria G.},
  date = {2021-03-31},
  journaltitle = {European Respiratory Review},
  shortjournal = {Eur Respir Rev},
  volume = {30},
