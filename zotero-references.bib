@article{beersWhenAlveolarType2017,
  title = {When {{Is}} an {{Alveolar Type}} 2 {{Cell}} an {{Alveolar Type}} 2 {{Cell}}? {{A Conundrum}} for {{Lung Stem Cell Biology}} and {{Regenerative Medicine}}},
  shorttitle = {When {{Is}} an {{Alveolar Type}} 2 {{Cell}} an {{Alveolar Type}} 2 {{Cell}}?},
  author = {Beers, Michael F. and Moodley, Yuben},
  date = {2017-07},
  journaltitle = {American Journal of Respiratory Cell and Molecular Biology},
  shortjournal = {Am J Respir Cell Mol Biol},
