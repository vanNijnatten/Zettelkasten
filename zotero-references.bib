@article{parkFunctionalSignificanceApoptosis2007,
  title = {Functional {{Significance}} of {{Apoptosis}} in {{Chronic Obstructive Pulmonary Disease}}},
  author = {Park, Jeong-Woong and Ryter, Stefan W. and Choi, Augustine M. K.},
  date = {2007-01},
  journaltitle = {COPD: Journal of Chronic Obstructive Pulmonary Disease},
  shortjournal = {COPD: Journal of Chronic Obstructive Pulmonary Disease},
  volume = {4},
