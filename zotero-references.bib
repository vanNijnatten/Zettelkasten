@article{limShapingHumanTelomeres2021,
  title = {Shaping Human Telomeres: From Shelterin and {{CST}} Complexes to Telomeric Chromatin Organization},
  shorttitle = {Shaping Human Telomeres},
  author = {Lim, Ci Ji and Cech, Thomas R.},
  date = {2021-04},
  journaltitle = {Nature Reviews Molecular Cell Biology},
