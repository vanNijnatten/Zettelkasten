@article{herbertUsingMultipleOnline2015,
  title = {Using Multiple Online Databases to Help Identify {{microRNAs}} Regulating the Airway Epithelial Cell Response to a Virus-like Stimulus: {{microRNA}} and Epithelial Cell Responses},
