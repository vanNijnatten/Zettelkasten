@article{thaiRegulationAirwayMucin2008,
  title = {Regulation of {{Airway Mucin Gene Expression}}},
  author = {Thai, Philip and Loukoianov, Artem and Wachi, Shinichiro and Wu, Reen},
  date = {2008-03-01},
  journaltitle = {Annual Review of Physiology},
  shortjournal = {Annu. Rev. Physiol.},
  volume = {70},
  number = {1},
  pages = {405--429},
  issn = {0066-4278, 1545-1585},
  doi = {10.1146/annurev.physiol.70.113006.100441},
  url = {https://www.annualreviews.org/doi/10.1146/annurev.physiol.70.113006.100441},
  urldate = {2021-11-18},
  abstract = {Mucins are important components that exert a variety of functions in cell-cell interaction, epidermal growth factor receptor signaling, and airways protection. In the conducting airways of the lungs, mucins are the major contributor to the viscoelastic property of mucous secretion, which is the major barrier to trapping inhaled microbial organism, particulates, and oxidative pollutants. The homeostasis of mucin production is an important feature in conducting airways for the maintenance of mucociliary function. Aberrant mucin secretion and accumulation in airway lumen are clinical hallmarks associated with various lung diseases, such as asthma, chronic obstructive pulmonary disease, cystic fibrosis, emphysema, and lung cancer. Among 20 known mucin genes identified, 11 of them have been verified at either the mRNA and/or protein level in airways. The regulation of mucin genes is complicated, as are the mediators and signaling pathways. This review summarizes the current view on the mediators, the signaling pathways, and the transcriptional units that are involved in the regulation of airway mucin gene expression. In addition, we also point out essential features of epigenetic mechanisms for the regulation of these genes.},
  langid = {english},
  keywords = {archived},
  annotation = {https://web.archive.org/web/20211118123411/https://www.annualreviews.org/doi/10.1146/annurev.physiol.70.113006.100441},
  file = {/Users/vannijnatten/Zotero/storage/7XN5D5SP/Thai et al_2008_Regulation of Airway Mucin Gene Expression.pdf}
}

@article{tharpAdhesionmediatedMechanosignalingForces2021,
  title = {Adhesion-Mediated Mechanosignaling Forces Mitohormesis},
  author = {Tharp, Kevin M. and Higuchi-Sanabria, Ryo and Timblin, Greg A. and Ford, Breanna and Garzon-Coral, Carlos and Schneider, Catherine and Muncie, Jonathon M. and Stashko, Connor and Daniele, Joseph R. and Moore, Andrew S. and Frankino, Phillip A. and Homentcovschi, Stefan and Manoli, Sagar S. and Shao, Hao and Richards, Alicia L. and Chen, Kuei-Ho and ten Hoeve, Johanna and Ku, Gregory M. and Hellerstein, Marc and Nomura, Daniel K. and Saijo, Karou and Gestwicki, Jason and Dunn, Alexander R. and Krogan, Nevan J. and Swaney, Danielle L. and Dillin, Andrew and Weaver, Valerie M.},
