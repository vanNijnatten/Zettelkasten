  author = {Silverman, Edwin K. and Chapman, Harold A. and Drazen, Jeffrey M. and Weiss, Scott T. and Rosner, Bernard and Campbell, Edward J. and O'Donnell, Walter J. and Reilly, John J. and Ginns, Leo and Mentzer, Steven and Wain, John and Speizer, Frank E.},
  date = {1998-06},
  journaltitle = {American Journal of Respiratory and Critical Care Medicine},
  shortjournal = {Am J Respir Crit Care Med},
  volume = {157},
