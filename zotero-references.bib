@article{morrisonEvaluationWholegenomeDNA2021,
  title = {Evaluation of Whole-Genome {{DNA}} Methylation Sequencing Library Preparation Protocols},
  author = {Morrison, Jacob and Koeman, Julie M. and Johnson, Benjamin K. and Foy, Kelly K. and Beddows, Ian and Zhou, Wanding and Chesla, David W. and Rossell, Larissa L. and Siegwald, Emily J. and Adams, Marie and Shen, Hui},
  date = {2021-12},
  journaltitle = {Epigenetics \& Chromatin},
  shortjournal = {Epigenetics \& Chromatin},
  volume = {14},
