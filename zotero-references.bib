  author = {Martinez, Fernando J. and Curtis, Jeffrey L. and Sciurba, Frank and Mumford, Jeanette and Giardino, Nicholas D. and Weinmann, Gail and Kazerooni, Ella and Murray, Susan and Criner, Gerard J. and Sin, Donald D. and Hogg, James and Ries, Andrew L. and Han, MeiLan and Fishman, Alfred P. and Make, Barry and Hoffman, Eric A. and Mohsenifar, Zab and Wise, Robert and {for the National Emphysema Treatment Trial Research Group*}},
  date = {2007-08},
