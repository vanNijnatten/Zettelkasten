  author = {Kuo, Chih-Hsi Scott and Pavlidis, Stelios and Loza, Matthew and Baribaud, Fred and Rowe, Anthony and Pandis, Ioannis and Hoda, Uruj and Rossios, Christos and Sousa, Ana and Wilson, Susan J. and Howarth, Peter and Dahlen, Barbro and Dahlen, Sven-Erik and Chanez, Pascal and Shaw, Dominick and Krug, Norbert and Sandstrӧm, Thomas and De Meulder, Bertrand and Lefaudeux, Diane and Fowler, Stephen and Fleming, Louise and Corfield, Julie and Auffray, Charles and Sterk, Peter J. and Djukanovic, Ratko and Guo, Yike and Adcock, Ian M. and Chung, Kian Fan},
  date = {2017-02-15},
  journaltitle = {American Journal of Respiratory and Critical Care Medicine},
