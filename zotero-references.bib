@article{langeEffectsSmokingChanges1989,
  title = {Effects of Smoking and Changes in Smoking Habits on the Decline of {{FEV1}}},
  author = {Lange, P. and Groth, S. and Nyboe, G. J. and Mortensen, J. and Appleyard, M. and Jensen, G. and Schnohr, P.},
