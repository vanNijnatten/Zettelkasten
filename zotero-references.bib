}

@article{mirzaCOPDGuidelinesReview2018,
  title = {{{COPD Guidelines}}: {{A Review}} of the 2018 {{GOLD Report}}},
  shorttitle = {{{COPD Guidelines}}},
  author = {Mirza, Shireen and Clay, Ryan D. and Koslow, Matthew A. and Scanlon, Paul D.},
