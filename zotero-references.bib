  author = {Szklarczyk, Damian and Gable, Annika L. and Lyon, David and Junge, Alexander and Wyder, Stefan and Huerta-Cepas, Jaime and Simonovic, Milan and Doncheva, Nadezhda T. and Morris, John H. and Bork, Peer and Jensen, Lars J. and von Mering, Christian},
