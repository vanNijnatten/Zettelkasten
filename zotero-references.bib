@article{miravitllesReviewNationalGuidelines2016,
  title = {A Review of National Guidelines for Management of {{COPD}} in {{Europe}}},
  author = {Miravitlles, Marc and Vogelmeier, Claus and Roche, Nicolas and Halpin, David and Cardoso, João and Chuchalin, Alexander G. and Kankaanranta, Hannu and Sandström, Thomas and Śliwiński, Paweł and Zatloukal, Jaromir and Blasi, Francesco},
  date = {2016-02},
  journaltitle = {European Respiratory Journal},
  shortjournal = {Eur Respir J},
  volume = {47},
  number = {2},
  pages = {625--637},
  issn = {0903-1936, 1399-3003},
  doi = {10.1183/13993003.01170-2015},
  url = {http://erj.ersjournals.com/lookup/doi/10.1183/13993003.01170-2015},
  urldate = {2022-03-12},
  abstract = {The quality of care can be improved by the development and implementation of evidence-based treatment guidelines. Different national guidelines for chronic obstructive pulmonary disease (COPD) exist in Europe and relevant differences may exist among them.                            This was an evaluation of COPD treatment guidelines published in Europe and Russia in the past 7\hspace{0.25em}years. Each guideline was reviewed in detail and information about the most important aspects of patient diagnosis, risk stratification and pharmacotherapy was extracted following a standardised process. Guidelines were available from the Czech Republic, England and Wales, Finland, France, Germany, Italy, Poland, Portugal, Russia, Spain and Sweden. The treatment goals, criteria for COPD diagnosis, consideration of comorbidities in treatment selection and support for use of long-acting bronchodilators, were similar across treatment guidelines. There were differences in measures used for stratification of disease severity, consideration of patient phenotypes, criteria for the use of inhaled corticosteroids and recommendations for other medications (               e.g.               theophylline and mucolytics) in addition to bronchodilators.                          There is generally good agreement on treatment goals, criteria for diagnosis of COPD and use of long-acting bronchodilators as the cornerstone of treatment among guidelines for COPD management in Europe and Russia. However, there are differences in the definitions of patient subgroups and other recommended treatments.},
  langid = {english},
  keywords = {archived,REVIEW},
  annotation = {https://web.archive.org/web/20220312001144/https://erj.ersjournals.com/content/47/2/625},
  file = {/Users/vannijnatten/Zotero/storage/LZQAQZLU/Miravitlles et al_2016_A review of national guidelines for management of COPD in Europe.pdf}
}

@article{miravitllesUnderstandingImpactSymptoms2017,
