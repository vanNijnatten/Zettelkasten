@article{lindenRespiratoryViralInfection2019,
  title = {Respiratory Viral Infection: A Potential ‚ÄúMissing Link‚Äù in the Pathogenesis of {{COPD}}},
  shorttitle = {Respiratory Viral Infection},
  author = {Linden, Dermot and Guo-Parke, Hong and Coyle, Peter V. and Fairley, Derek and McAuley, Danny~F. and Taggart, Clifford C. and Kidney, Joe},
  date = {2019-03-31},
  journaltitle = {European Respiratory Review},
