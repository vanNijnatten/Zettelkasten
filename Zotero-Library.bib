  file = {/Users/vannijnatten/Zotero/storage/ZU8SU8VG/Steiling et al_2013_A Dynamic Bronchial Airway Gene Expression Signature of Chronic Obstructive.pdf}
}

@article{stewartExtremeHeterogeneityHuman2021,
  title = {Extreme Heterogeneity of Human Mitochondrial {{DNA}} from Organelles to Populations},
  author = {Stewart, James B. and Chinnery, Patrick F.},
  date = {2021-02},
  journaltitle = {Nature Reviews Genetics},
  shortjournal = {Nat Rev Genet},
  volume = {22},
  number = {2},
  pages = {106--118},
  issn = {1471-0056, 1471-0064},
  doi = {10.1038/s41576-020-00284-x},
  url = {https://www.nature.com/articles/s41576-020-00284-x},
  urldate = {2022-03-13},
  abstract = {Contrary to the long-held view that most humans harbour only identical mitochondrial genomes (mtDNA), deep re-sequencing has uncovered unanticipated extreme genetic variation. Most, if not all humans, contain multiple mtDNA genotypes (heteroplasmy), specific patterns of variants accumulate in different tissues over time (including cancers), and some are preferentially passed down or suppressed in the maternal germ line. These findings cast light on the origin and spread of mtDNA mutations at multiple scales, from the organelle to the human population, and challenge the conventional view that high percentage levels of a mutation are required before a new variant has functional consequences.},
  langid = {english},
  keywords = {archived,REVIEW},
  annotation = {https://web.archive.org/web/20220313200937/https://www.nature.com/articles/s41576-020-00284-x},
  note = {Version URL: {$<$}a href="https://web.archive.org/web/20220313200937/https://www.nature.com/articles/s41576-020-00284-x" data-originalurl="https://www.nature.com/articles/s41576-020-00284-x" data-versiondate="2022-03-13T20:09:37Z"{$>$}Robust Link for: https://www.nature.com/articles/s41576-020-00284-x{$<$}/a{$>$}},
  file = {/Users/vannijnatten/Zotero/storage/UZ7NUFEZ/Stewart_Chinnery_2021_Extreme heterogeneity of human mitochondrial DNA from organelles to populations.pdf}
}

@report{sudermanDmrffIdentifyingDifferentially2018,
  type = {preprint},
  title = {Dmrff: Identifying Differentially Methylated Regions Efficiently with Power and Control},
  shorttitle = {Dmrff},
  author = {Suderman, Matthew and Staley, James R and French, Robert and Arathimos, Ryan and Simpkin, Andrew and Tilling, Kate},
  date = {2018-12-31},
  institution = {{Bioinformatics}},
  doi = {10.1101/508556; https://web.archive.org/web/20220311213841/https://www.biorxiv.org/content/10.1101/508556v1},
  url = {http://biorxiv.org/lookup/doi/10.1101/508556},
  urldate = {2022-03-11},
  abstract = {A differentially methylated region (DMR) is a genomic region in which DNA methylation is consistently positively or negatively associated with a phenotype or exposure. We demonstrate that existing algorithms for identifying DMRs either fail to consistently control false positive rates (comb-p and DMRcate), suffer from low power (bumphunter) or lack modeling flexibility (seqlm). We introduce a new method, dmrff, that overcomes these shortcomings and can additionally be used to meta-analyze multiple datasets. When applied to investigate associations of age in multiple datasets, dmrff identifies novel DMRs near genes previously linked to age. An R implementation is available on Github (http://github.com/perishky/dmrff).},
  langid = {english},
  keywords = {üõ†,archived},
