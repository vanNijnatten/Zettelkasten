  author = {Aliahmad, Parinaz and Kadavallore, Asha and de la Torre, Brian and Kappes, Dietmar and Kaye, Jonathan},
  options = {useprefix=true},
  date = {2011-12-01},
  journaltitle = {The Journal of Immunology},
