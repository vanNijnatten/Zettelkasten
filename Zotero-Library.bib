@article{hanssonEthicsBiobanks2004,
  title = {The {{Ethics}} of {{Biobanks}}},
  author = {Hansson, Sven Ove},
  date = {2004-10},
  journaltitle = {Cambridge Quarterly of Healthcare Ethics},
  shortjournal = {Cambridge Q. Healthcare Ethics},
  volume = {13},
  number = {04},
  issn = {0963-1801, 1469-2147},
  doi = {10.1017/S0963180104134038},
  url = {http://www.journals.cambridge.org/abstract_S0963180104134038},
  urldate = {2022-03-11},
  abstract = {Due to modern biochemistry and, in particular, recent developments in genomics, proteomics, and bioinformatics, human samples (organs, tissues, cells, genes, etc.) have become the most important raw materials for advancement in the health sciences. Such material has been at the center of fundamental biomedical research for a long time. What is new is its increased usefulness in research with direct clinical relevance, such as the development of drugs. Because of the larger commercial involvement in such research, this has also led to greater economic interests in human biological material and in the information that can be extracted from it. Classical distinctions between different types of biological materials, such as organs, tissues, cells, and molecules, have to be reconsidered in the light of ongoing biotechnological developments. In particular, the therapeutic use of pluripotent cells makes the distinction between organs, tissues, and cells less relevant in some applications.1 Similarly, DNA sequencing makes the distinction between biological material and the information obtainable from it less clear. For many purposes, it is no longer important if one has the actual DNA or the information about its composition. DNA information from populations is currently one of the most important forms of biobanks. DNA can be obtained with simple methods, such as mouth washing, that are suitable for mass collection of samples. The value of a DNA bank increases with the social and medical information to which it is coupled. If it is combined, for instance, with electronic health records for each subject, then correlations can be searched for between genes, environmental (‚Äúlifestyle‚Äù) factors, and illness. Because both genetic and lifestyle information is privacy sensitive, the requirements of privacy and security in such research are substantial. The present contribution provides an overview of the ethical issues that are connected with the collection and use of biobanks. After a brief summary of current practices, a systematic review is made of the three major groups of ethical issues put forth. I then discuss the need for internationally harmonized regulations and a stronger focus on patient perspectives in future discussions on biobank ethics.},
  langid = {english},
  keywords = {ethics},
  file = {/Users/vannijnatten/Zotero/storage/7PTNKJG8/Hansson_2004_The Ethics of Biobanks.pdf}
}

@article{hanzelmannGSVAGeneSet2013,
