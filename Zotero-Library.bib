}

@article{wangPulmonaryAlveolarType2018,
  title = {Pulmonary Alveolar Type {{I}} Cell Population Consists of Two Distinct Subtypes That Differ in Cell Fate},
  author = {Wang, Yanjie and Tang, Zan and Huang, Huanwei and Li, Jiao and Wang, Zheng and Yu, Yuanyuan and Zhang, Chengwei and Li, Juan and Dai, Huaping and Wang, Fengchao and Cai, Tao and Tang, Nan},
  date = {2018-03-06},
  journaltitle = {Proceedings of the National Academy of Sciences},
  shortjournal = {Proc. Natl. Acad. Sci. U.S.A.},
  volume = {115},
  number = {10},
  pages = {2407--2412},
  issn = {0027-8424, 1091-6490},
  doi = {10.1073/pnas.1719474115},
  url = {https://pnas.org/doi/full/10.1073/pnas.1719474115},
  urldate = {2022-03-21},
  abstract = {Significance                            Pulmonary alveolar type I (AT1) cells are essential for the gas-exchange function of lungs. AT1 cells retain their cellular plasticity during injury-induced alveolar regeneration. However, we know very little about the developmental heterogeneity of the AT1 cell population. Our study identified a robust genetic marker of postnatal AT1 cells, insulin-like growth factor-binding protein 2 (Igfbp2). We use this marker to demonstrate that the postnatal AT1 cell population actually consists of two AT1 cell subtypes (Hopx               +               Igfbp2               +               and Hopx               +               Igfbp2               ‚àí               AT1 cells) with distinct cell fates during alveolar regeneration. The large majority of adult AT1 cells expresses Igfbp2 and cannot transdifferentiate into AT2 cells during post pneumonectomy formation of new alveoli. Therefore, Hopx               +               Igfbp2               +               AT1 cells represent the terminally differentiated population of AT1 cells.                        ,                             Pulmonary alveolar type I (AT1) cells cover more than 95\% of alveolar surface and are essential for the air‚Äìblood barrier function of lungs. AT1 cells have been shown to retain developmental plasticity during alveolar regeneration. However, the development and heterogeneity of AT1 cells remain largely unknown. Here, we conducted a single-cell RNA-seq analysis to characterize postnatal AT1 cell development and identified insulin-like growth factor-binding protein 2 (Igfbp2) as a genetic marker specifically expressed in postnatal AT1 cells. The portion of AT1 cells expressing Igfbp2 increases during alveologenesis and in post pneumonectomy (PNX) newly formed alveoli. We found that the adult AT1 cell population contains both Hopx               +               Igfbp2               +               and Hopx               +               Igfbp2               ‚àí               AT1 cells, which have distinct cell fates during alveolar regeneration. Using an               Igfbp2               -CreER mouse model, we demonstrate that Hopx               +               Igfbp2               +               AT1 cells represent terminally differentiated AT1 cells that are not able to transdifferentiate into AT2 cells during post-PNX alveolar regeneration. Our study provides tools and insights that will guide future investigations into the molecular and cellular mechanism or mechanisms underlying AT1 cell fate during lung development and regeneration.},
  langid = {english},
  file = {/Users/vannijnatten/Zotero/storage/PYPGESUM/Wang et al_2018_Pulmonary alveolar type I cell population consists of two distinct subtypes.pdf}
}

