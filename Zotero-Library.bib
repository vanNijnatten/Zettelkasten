@article{herbertUsingMultipleOnline2015,
  title = {Using Multiple Online Databases to Help Identify {{microRNAs}} Regulating the Airway Epithelial Cell Response to a Virus-like Stimulus: {{microRNA}} and Epithelial Cell Responses},
  shorttitle = {Using Multiple Online Databases to Help Identify {{microRNAs}} Regulating the Airway Epithelial Cell Response to a Virus-like Stimulus},
  author = {Herbert, Cristan and Sebesfi, Michelle and Zeng, Qing-Xiang and Oliver, Brian G and Foster, Paul S and Kumar, Rakesh K},
  date = {2015-11},
  journaltitle = {Respirology},
