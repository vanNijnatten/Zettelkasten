@article{mcgeachieCGBayesNetsConditionalGaussian2014,
  title = {{{CGBayesNets}}: {{Conditional Gaussian Bayesian Network Learning}} and {{Inference}} with {{Mixed Discrete}} and {{Continuous Data}}},
  shorttitle = {{{CGBayesNets}}},
  author = {McGeachie, Michael J. and Chang, Hsun-Hsien and Weiss, Scott T.},
