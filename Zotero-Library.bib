@article{jonesDevelopmentFirstValidation2009,
  title = {Development and First Validation of the {{COPD Assessment Test}}},
  author = {Jones, P. W. and Harding, G. and Berry, P. and Wiklund, I. and Chen, W-H. and Kline Leidy, N.},
  date = {2009-09-01},
  journaltitle = {European Respiratory Journal},
