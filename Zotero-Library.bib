@article{woutersBurdenCOPDNetherlands2003,
  title = {The Burden of {{COPD}} in {{The Netherlands}}: Results from the {{Confronting COPD}} Survey},
  shorttitle = {The Burden of {{COPD}} in {{The Netherlands}}},
  author = {Wouters, E. F. M.},
  date = {2003-03},
  journaltitle = {Respiratory Medicine},
  shortjournal = {Respir Med},
  volume = {97 Suppl C},
  eprint = {12647943},
  eprinttype = {pmid},
  pages = {S51-59},
  issn = {0954-6111},
  doi = {10.1016/s0954-6111(03)80025-2},
  abstract = {Chronic obstructive pulmonary disease (COPD) is a disabling condition associated with progressive airflow limitation that is largely irreversible, with symptoms of dyspnoea, cough and sputum production. In The Netherlands, COPD tends to be underpresented to physicians, underdiagnosed and undertreated by healthcare professionals, and poorly recognized among the general population. Improving the diagnosis and management of COPD in this country may require raised awareness of the impact of the disease on society. This may be achieved by providing detailed information on the burden of COPD on the patient, healthcare system and the economy. This information has now become available from Confronting COPD in North America and Europe, the first international survey to quantify the country-specific impact of the disease. An economic analysis of the results of the Dutch survey revealed the high cost of COPD to the healthcare system and society, with direct costs estimated at Euro 614 per patient Indirect costs amounted to Euro 410, bringing the annual per patient cost of COPD to Euro 1024. Around 50\% of the cost of COPD to the healthcare system was for prescription medication, including symptomatic medication and treatment for underlying airway inflammation. This contributed to effective symptom control in many patients, as shown by the low utilization of unscheduled healthcare (inpatient hospitalizations, emergency room visits, or contacts with healthcare professionals). However, the survey suggested that there was scope for improvement in the understanding of the origin and consequences of this disease among the general public, and the way COPD is managed by healthcare professionals, in order to increase the proportion of patients who are diagnosed with COPD and treated in accordance with management guidelines. In addition, smoking cessation intervention at the early stages of the disease could help to reduce the high costs associated with severe COPD in this country.},
  langid = {english},
  file = {/Users/vannijnatten/Zotero/storage/5U9FJJQX/Wouters_2003_The burden of COPD in The Netherlands.pdf}
}

@article{xuDNAMethylationChildhood2018,
  title = {{{DNA}} Methylation in Childhood Asthma: An Epigenome-Wide Meta-Analysis},
  shorttitle = {{{DNA}} Methylation in Childhood Asthma},
  author = {Xu, Cheng-Jian and S√∂derh√§ll, Cilla and Bustamante, Mariona and Ba√Øz, Nour and Gruzieva, Olena and Gehring, Ulrike and Mason, Dan and Chatzi, Leda and Basterrechea, Mikel and Llop, Sabrina and Torrent, Maties and Forastiere, Francesco and Fantini, Maria Pia and Carlsen, Karin C L√∏drup and Haahtela, Tari and Morin, Andr√©anne and Kerkhof, Marjan and Merid, Simon Kebede and van Rijkom, Bianca and Jankipersadsing, Soesma A and Bonder, Marc Jan and Ballereau, Stephane and Vermeulen, Cornelis J and Aguirre-Gamboa, Raul and de Jongste, Johan C and Smit, Henriette A and Kumar, Ashish and Pershagen, G√∂ran and Guerra, Stefano and Garcia-Aymerich, Judith and Greco, Dario and Reinius, Lovisa and McEachan, Rosemary R C and Azad, Raf and Hovland, Vegard and Mowinckel, Petter and Alenius, Harri and Fyhrquist, Nanna and Lemonnier, Nathana√´l and Pellet, Johann and Auffray, Charles and van der Vlies, Pieter and van Diemen, Cleo C and Li, Yang and Wijmenga, Cisca and Netea, Mihai G and Moffatt, Miriam F and Cookson, William O C M and Anto, Josep M and Bousquet, Jean and Laatikainen, Tiina and Laprise, Catherine and Carlsen, Kai-H√•kon and Gori, Davide and Porta, Daniela and I√±iguez, Carmen and Bilbao, Jose Ramon and Kogevinas, Manolis and Wright, John and Brunekreef, Bert and Kere, Juha and Nawijn, Martijn C and Annesi-Maesano, Isabella and Sunyer, Jordi and Mel√©n, Erik and Koppelman, Gerard H},
  options = {useprefix=true},
  date = {2018-05},
  journaltitle = {The Lancet Respiratory Medicine},
  shortjournal = {The Lancet Respiratory Medicine},
