@article{tasenaMiR315p2020,
  title = {{{MiR}}‚Äê31‚Äê5p: {{A}} Shared Regulator of Chronic Mucus Hypersecretion in Asthma and Chronic Obstructive Pulmonary Disease},
  shorttitle = {{{MiR}}‚Äê31‚Äê5p},
  author = {Tasena, Hataitip and Boudewijn, Ilse M. and Faiz, Alen and Timens, Wim and Hylkema, Machteld N. and Berg, Marijn and Hacken, Nick H. T. and Brandsma, Corry‚ÄêAnke and Heijink, Irene H. and den Berge, Maarten},
  options = {useprefix=true},
