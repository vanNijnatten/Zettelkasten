@article{theglucoldstudygroupSmokingCessationBronchial2007,
  title = {Smoking Cessation and Bronchial Epithelial Remodelling in {{COPD}}: A Cross-Sectional Study},
  shorttitle = {Smoking Cessation and Bronchial Epithelial Remodelling in {{COPD}}},
  author = {{the GLUCOLD Study Group} and Lapperre, Th√©r√®se S and Sont, Jacob K and van Schadewijk, Annemarie and Gosman, Margot ME and Postma, Dirkje S and Bajema, Ingeborg M and Timens, Wim and Mauad, Thais and Hiemstra, Pieter S},
  options = {useprefix=true},
  date = {2007-12},
  journaltitle = {Respiratory Research},
  shortjournal = {Respir Res},
  volume = {8},
  number = {1},
  pages = {85},
  issn = {1465-993X},
  doi = {10.1186/1465-9921-8-85},
  url = {http://respiratory-research.biomedcentral.com/articles/10.1186/1465-9921-8-85},
  urldate = {2022-03-12},
  abstract = {Background: Chronic Obstructive Pulmonary Disease (COPD) is associated with bronchial epithelial changes, including squamous cell metaplasia and goblet cell hyperplasia. These features are partially attributed to activation of the epidermal growth factor receptor (EGFR). Whereas smoking cessation reduces respiratory symptoms and lung function decline in COPD, inflammation persists. We determined epithelial proliferation and composition in bronchial biopsies from current and ex-smokers with COPD, and its relation to duration of smoking cessation. Methods: 114 COPD patients were studied cross-sectionally: 99 males/15 females, age 62 ¬± 8 years, median 42 pack-years, no corticosteroids, current (n = 72) or ex-smokers (n = 42, median cessation duration 3.5 years), postbronchodilator FEV1 63 ¬± 9\% predicted. Squamous cell metaplasia (\%), goblet cell (PAS/Alcian Blue+) area (\%), proliferating (Ki-67+) cell numbers (/mm basement membrane), and EGFR expression (\%) were measured in intact epithelium of bronchial biopsies. Results: Ex-smokers with COPD had significantly less epithelial squamous cell metaplasia, proliferating cell numbers, and a trend towards reduced goblet cell area than current smokers with COPD (p = 0.025, p = 0.001, p = 0.081, respectively), but no significant difference in EGFR expression. Epithelial features were not different between short-term quitters ({$<$}3.5 years) and current smokers. Long-term quitters (‚â•3.5 years) had less goblet cell area than both current smokers and short-term quitters (medians: 7.9\% vs. 14.4\%, p = 0.005; 7.9\% vs. 13.5\%, p = 0.008; respectively), and less proliferating cell numbers than current smokers (2.8\% vs. 18.6\%, p {$<$} 0.001). Conclusion: Ex-smokers with COPD had less bronchial epithelial remodelling than current smokers, which was only observed after long-term smoking cessation ({$>$}3.5 years).},
  langid = {english},
  keywords = {archived},
  annotation = {https://web.archive.org/web/20220312001239/https://respiratory-research.biomedcentral.com/articles/10.1186/1465-9921-8-85},
  note = {Version URL: {$<$}a href="https://web.archive.org/web/20220312001239/https://respiratory-research.biomedcentral.com/articles/10.1186/1465-9921-8-85" data-originalurl="http://respiratory-research.biomedcentral.com/articles/10.1186/1465-9921-8-85" data-versiondate="2022-03-12T00:12:39Z"{$>$}Robust Link for: http://respiratory-research.biomedcentral.com/articles/10.1186/1465-9921-8-85{$<$}/a{$>$}},
  file = {/Users/vannijnatten/Zotero/storage/VTHH9A98/the GLUCOLD Study Group et al_2007_Smoking cessation and bronchial epithelial remodelling in COPD.pdf}
}

@article{thumExpressionXenobioticMetabolizing2006,
  title = {Expression of {{Xenobiotic Metabolizing Enzymes}} in {{Different Lung Compartments}} of {{Smokers}} and {{Nonsmokers}}},
