  number = {4},
  pages = {552--561},
  issn = {1940-6207, 1940-6215},
  doi = {10.1158/1940-6207.CAPR-10-0183},
  url = {http://cancerpreventionresearch.aacrjournals.org/lookup/doi/10.1158/1940-6207.CAPR-10-0183},
  urldate = {2022-03-12},
  abstract = {Lung cancer is the leading cause of cancer death worldwide. Accurate prediction of lung cancer risk is of value for individuals, clinicians, and researchers. The aims of this study were to characterize the associations between pulmonary function and sputum DNA image cytometry (SDIC) and lung cancer, and their contributions to risk prediction. During 1990 to 2007, 2,596 high-risk individuals were enrolled and followed prospectively for development of lung cancer (n ¬º 139; median follow-up 7.7 years) in trials at the British Columbia Cancer Agency. At baseline, an epidemiologic questionnaire was administered, sputum was collected for aneuploidy measurement and spirometry was obtained. Multivariable logistic models were prepared including known lung cancer predictors (model 1), that additionally included percent-expected-forced expiratory volume in 1 second [forced expiratory volume in 1 second (FEV1\%), model 2], and that additionally included SDIC (model 3). Prediction was assessed by evaluating discrimination (receiver operator characteristic area under the curve (ROC AUC)) and calibration. Net reclassification indices (NRI) were calculated with cutoff points for 8-year risks identifying low, intermediate, and high risk at 1.5\% and 3\%. Lung cancer risk increased with decline in FEV1\%, but did so more for men than for women (interaction P {$<$} 0.001). SDIC demonstrated a dose‚Äìresponse with lung cancer (P ¬º 0.022). The ROC AUCs for models 1, 2, and 3 were 0.718 (95\% CI: 0.671‚Äì0.765), 0.767 (95\% CI: 0.725‚Äì0.809), and 0.773 (95\% CI: 0.732‚Äì0.815), respectively. Model 2 versus 1 had a NRI of 12.6\% (P {$<$} 0.0001) and model 3 versus 2 had a NRI of 3.1\% (P ¬º 0.059). Spirometry and SDIC data substantially and minimally improved lung cancer prediction, respectively.},
  langid = {english},
  keywords = {archived},
  annotation = {https://web.archive.org/web/20220312000515/https://aacrjournals.org/cancerpreventionresearch/article/4/4/552/31876/Incremental-Value-of-Pulmonary-Function-and-Sputum},
  note = {Version URL: {$<$}a href="https://web.archive.org/web/20220312000515/https://aacrjournals.org/cancerpreventionresearch/article/4/4/552/31876/Incremental-Value-of-Pulmonary-Function-and-Sputum" data-originalurl="http://cancerpreventionresearch.aacrjournals.org/lookup/doi/10.1158/1940-6207.CAPR-10-0183" data-versiondate="2022-03-12T00:05:15Z"{$>$}Robust Link for: http://cancerpreventionresearch.aacrjournals.org/lookup/doi/10.1158/1940-6207.CAPR-10-0183{$<$}/a{$>$}},
  file = {/Users/vannijnatten/Zotero/storage/5SKNCBK9/Tammemagi et al_2011_Incremental Value of Pulmonary Function and Sputum DNA Image Cytometry in Lung.pdf}
}

@article{tanUsingBayesianNetworks2012,
  title = {Using {{Bayesian Networks}} to {{Construct Gene Regulatory Networks}} from {{Microarray Data}}},
  author = {Tan, Ai Kung and Mohamad, Mohd Saberi},
  date = {2012-11-02},
  journaltitle = {Jurnal Teknologi},
  shortjournal = {Jurnal Teknologi},
  pages = {1--6},
  issn = {2180-3722, 0127-9696},
  doi = {10.11113/jt.v58.1255},
  url = {https://journals.utm.my/index.php/jurnalteknologi/article/view/1255},
  urldate = {2022-03-11},
  abstract = {In this research, Bayesian network is proposed as the model to construct gene regulatory networks from Saccharomyces cerevisiae cell-cycle gene expression dataset and Escherichia coli dataset due to its capability of handling microarray datasets with missing values. The goal of this research is to study and to understand the framework of the Bayesian networks, and then to construct gene regulatory networks from Saccharomyces cerevisiae cell-cycle gene expression dataset and Escherichia coli dataset by developing Bayesian networks using hill-climbing algorithm and Efron‚Äôs bootstrap approach and then the performance of the constructed gene networks of Saccharomyces cerevisiae are evaluated and are compared with the previously constructed sub-networks by Dejori [14]. At the end of this research, the gene networks constructed for Saccharomyces cerevisiae not only have achieved high True Positive Rate (more than 90\%), but the networks constructed also have discovered more potential interactions between genes. Therefore, it can be concluded that the performance of the gene regulatory networks constructed using Bayesian networks in this research is proved to be better because it can reveal more gene relationships. 				 				Dalam penyelidikan ini, Bayesian network adalah dicadangkan sebagai model untuk membina gene regulatory networks dari kitar sel S. cerevisiae set data disebabkan keupayaannya untuk mengendali set data microarray yang mempunyai nilai-nilai yang hilang. Tujuan penyelidikan ini adalah untuk mempelajari dan memahami rekabentuk untuk Bayesian network, dan kemudian untuk membina gene regulatory networks dari data Saccharomyces cerevisiae cell-cycle gene expression dan data Escherichia coli dengan membina model Bayesian networks dengan menggunakan algoritma hill-climbing serta Efron‚Äôs bootstrap approach dan gene networks yang dibina untuk Saccharomyces cerevisiae dibandingkan dengan sub-networks yang dibina oleh Dejori [14]. Pada akhir kajian ini, gene networks yang dibina untuk Saccharomyces cerevisiae bukan sahaja telah mencapai True Positive Rate yang tinggi (lebih dari 90\%), tetapi gene networks yang dibina juga telah menemui lebih banyak interaksi berpotensi antara gen. Oleh kerana itu, dapat disimpulkan bahawa prestasi gene networks yang dibina menggunakan Bayesian network dalam kajian ini adalah terbukti lebih baik kerana ia boleh mendedahkan lebih banyak hubungan antara gen.},
  keywords = {archived},
  annotation = {https://web.archive.org/web/20220311210408/https://journals.utm.my/index.php/jurnalteknologi/article/view/1255},
  note = {Version URL: {$<$}a href="https://web.archive.org/web/20220311210408/https://journals.utm.my/index.php/jurnalteknologi/article/view/1255" data-originalurl="https://journals.utm.my/index.php/jurnalteknologi/article/view/1255" data-versiondate="2022-03-11T21:04:08Z"{$>$}Robust Link for: https://journals.utm.my/index.php/jurnalteknologi/article/view/1255{$<$}/a{$>$}},
  file = {/Users/vannijnatten/Zotero/storage/DFGHGIEV/Tan_Mohamad_2012_Using Bayesian Networks to Construct Gene Regulatory Networks from Microarray.pdf}
}

